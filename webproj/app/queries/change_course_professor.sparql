PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX : <http://hogwarts.edu/ontology.owl#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX hogwarts: <http://hogwarts.edu/ontology#>
PREFIX owl: <http://www.w3.org/2002/07/owl#>

DELETE {{
  ?course :hasProfessor ?old_professor .
}}
INSERT {{
  ?course :hasProfessor ?new_professor .
}}
WHERE {{
    BIND({course_id} AS ?course) .
    ?course :hasProfessor ?old_professor .
    BIND({professor_id} AS ?new_professor) .
}}